{
  "name": "twister-redux-starter",
  "version": "1.0.0",
  "description": "Starter project for twister-redux coding practice project",
  "main": "server/index.js",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "cookie": "^0.3.1",
    "express": "^4.14.0",
    "history": "^2.1.2",
    "lodash.isequal": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "query-string": "^4.2.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "^0.0.13",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.1",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-module-hot-accept": "^1.0.4"
  },
  "scripts": {
    "build:client": "webpack --config webpack.config.js",
    "build:server": "babel -d dist/server server && babel -d dist/client client",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint .",
    "dev": "better-npm-run dev",
    "start": "npm run build && better-npm-run start"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --watch server ./server/index.js --exec babel-node",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "node ./dist/server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesheepio/twister-redux-starter.git"
  },
  "author": "codesheep.io",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codesheepio/twister-redux-starter/issues"
  },
  "homepage": "https://github.com/codesheepio/twister-redux-starter#readme"
}
